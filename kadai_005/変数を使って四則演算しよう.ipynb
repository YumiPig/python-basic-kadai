      "source": [
        "# 上辺：10cm\n",
        "# 下辺：20cm\n",
        "# 高さ：5cm\n",
        "# 公式：台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "zyouhen = 10\n",
        "teihen = 20\n",
        "high = 5\n",
        "menseki = (zyouhen + teihen) * high / 2\n",
        "print(str(menseki), \"cm2\")"
      ]
    }
